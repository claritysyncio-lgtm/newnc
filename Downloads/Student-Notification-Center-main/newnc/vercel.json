{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "routes": [
    {
      "src": "/embed.html",
      "dest": "/embed.html"
    },
    {
      "src": "/legal.html",
      "dest": "/legal.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
